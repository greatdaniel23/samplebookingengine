# Console & API Configuration Flags
# Set VITE_FORCE_NO_CONSOLE=true to suppress all console output even in development
VITE_FORCE_NO_CONSOLE=false

# Set VITE_FORCE_RELATIVE_API=true to force relative /api base regardless of domain
VITE_FORCE_RELATIVE_API=false

# Override API base URL (defaults to https://api.rumahdaisycantik.com)
# VITE_API_BASE=https://api.rumahdaisycantik.com

# Override public base path (defaults to /)
# VITE_PUBLIC_BASE=/

# Override admin base route (defaults to /admin)
# VITE_ADMIN_BASE=/admin