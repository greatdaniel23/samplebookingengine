    setFormData(prev => {
      const updated = { ...prev, [field]: value };
      // Keep guests in sync with adults + children
      if (field === 'adults' || field === 'children') {
        updated.guests = (field === 'adults' ? value : updated.adults) + (field === 'children' ? value : updated.children);
      }
      return updated;
    });
  };

